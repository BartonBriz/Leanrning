#include <iostream>
#include <string.h>
using namespace std;
int main()
{
    string FirstName = "Michael";
    string LastName = "Barton";
    for (int i = 0; i < 10; i++ )
    {
        cout << "You first name: " << FirstName << "\n" << "You last name: " << LastName << endl;
    }
    return 0;
    
}
